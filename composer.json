{
	"name": "bumpcore/editor.php",
	"description": "PHP integration for Editor.js",
	"type": "library",
	"license": "MIT",
	"autoload": {
		"psr-4": {
			"BumpCore\\EditorPhp\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"BumpCore\\EditorPhp\\Tests\\": "tests/"
		}
	},
	"authors": [
		{
			"name": "Abdulkadir CemiloÄŸlu",
			"email": "kadir.cemiloglu1@gmail.com",
			"role": "Developer"
		}
	],
	"require": {
		"php": "^8.2",
		"illuminate/support": "^10.0|^11.0",
		"illuminate/validation": "^10.0|^11.0",
		"symfony/html-sanitizer": "^7.0"
	},
	"require-dev": {
		"pestphp/pest": "^2.0",
		"orchestra/testbench": "^9.0",
		"friendsofphp/php-cs-fixer": "^3.14.4",
		"phpstan/phpstan": "^1.10",
		"fakerphp/faker": "^1.23"
	},
	"suggest": {
		"fakerphp/faker": "Required to use fake data generation for blocks."
	},
	"config": {
		"allow-plugins": {
			"pestphp/pest-plugin": true
		}
	},
	"extra": {
		"laravel": {
			"providers": [
				"BumpCore\\EditorPhp\\EditorPhpServiceProvider"
			]
		}
	}
}

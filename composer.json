			"BumpCore\\EditorPhp\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"BumpCore\\EditorPhp\\Tests\\": "tests/"
		}
	},
	"authors": [
		{
			"name": "Abdulkadir CemiloÄŸlu",
			"email": "kadir.cemiloglu1@gmail.com",
			"role": "Developer"
		}
	],
	"require": {
		"php": "^8.1",
		"illuminate/support": "^9.0",
		"illuminate/validation": "^9.0"
	},
	"extra": {
		"laravel": {
			"providers": [
				"BumpCore\\EditorPhp\\EditorPhpServiceProvider"
			]
		}
	},
	"require-dev": {
		"pestphp/pest": "^1.22",
		"orchestra/testbench": "^7.19"
